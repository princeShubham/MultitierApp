@model IEnumerable<CommonLayer.Employee>


<h1>Index</h1>

<table class="table">

        <tr>
            <th>
                Id
            </th>
            <th>
              Name
            </th>
            <th>
                Email
            </th>
            <th>
                Dept
            </th>
            <th>
              Salary
            </th>
            <th></th>
        </tr>
   
@foreach (var item in Model) {
        <tr>
            <td>
                @item.Id
            </td>
            <td>
                @item.Name
            </td>
            <td>
                @item.Email
            </td>
            <td>
                @item.Dept
            </td>
            <td>
                @item.Salary
            </td>
          
        </tr>
}

</table>
